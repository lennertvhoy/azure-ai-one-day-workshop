{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "subscriptionId": {
            "value": "${AZ_SUBSCRIPTION_ID}"
        },
        "location": {
            "value": "${AZ_LOCATION:-westeurope}"
        },
        "resourceGroupName": {
            "value": "${LAB_NAME:-avd-lab}"
        },
        "namingPrefix": {
            "value": "avd"
        },
        "namingSuffix": {
            "value": "${LAB_NAME:-lab}"
        },
        "vmSize": {
            "value": "Standard_D2s_v3"
        },
        "vmImage": {
            "value": {
                "publisher": "MicrosoftWindowsDesktop",
                "offer": "windows-11",
                "sku": "win11-23h2-pro",
                "version": "latest"
            }
        },
        "hostPoolType": {
            "value": "Pooled"
        },
        "loadBalancerType": {
            "value": "BreadthFirst"
        },
        "maxSessionsPerHost": {
            "value": 10
        },
        "numberOfSessionHosts": {
            "value": 2
        },
        "vnetAddressPrefix": {
            "value": "10.0.0.0/16"
        },
        "subnetAddressPrefix": {
            "value": "10.0.0.0/24"
        },
        "aadJoinType": {
            "value": "AzureAD"
        },
        "tags": {
            "value": {
                "managed-by": "avd-lab-tool",
                "owner": "${OWNER:-lab-admin}",
                "course": "${COURSE:-azure-ai-workshop}",
                "env": "dev",
                "costCenter": "${COST_CENTER:-training}"
            }
        }
    }
}